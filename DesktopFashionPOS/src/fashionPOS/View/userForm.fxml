<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="child" prefHeight="606.0" prefWidth="1201.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fashionPOS.Controller.userController">
   <Label alignment="CENTER" prefHeight="70.0" prefWidth="1222.0" style="-fx-text-fill: #FFFFFF; -fx-background-color: #4b86b4;" text="Form Penambahan User" textAlignment="CENTER">
      <font>
         <Font size="48.0" />
      </font>
   </Label>
   <HBox>
      <VBox style="-fx-background-color: #adcbe3;">
         <GridPane alignment="CENTER" prefHeight="537.0" prefWidth="409.0" style="-fx-background-color: #adcbe3;">
            <columnConstraints>
               <ColumnConstraints hgrow="SOMETIMES" maxWidth="424.0" minWidth="10.0" prefWidth="245.0"/>
               <ColumnConstraints hgrow="SOMETIMES" maxWidth="828.0" minWidth="10.0" prefWidth="304.0"/>
            </columnConstraints>
            <rowConstraints>
               <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
               <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
               <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
               <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
               <RowConstraints maxHeight="104.0" minHeight="10.0" prefHeight="84.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <Label alignment="CENTER" prefHeight="41.0" prefWidth="230.0" text="Masukkan User ID"
                   textAlignment="CENTER">
               <font>
                  <Font size="17.0"/>
               </font>
            </Label>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="42.0" prefWidth="230.0"
                   text="Masukkan Username" textAlignment="CENTER" GridPane.rowIndex="1">
               <font>
                  <Font size="17.0"/>
               </font>
            </Label>
            <ComboBox fx:id="role" prefHeight="36.0" prefWidth="502.0" style="-fx-background-color: #eeeeee;"
                      GridPane.columnIndex="1" GridPane.rowIndex="3">
               <GridPane.margin>
                  <Insets left="15.0" right="15.0"/>
               </GridPane.margin>
            </ComboBox>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="42.0" prefWidth="230.0"
                   text="Masukkan Password" textAlignment="CENTER" GridPane.rowIndex="2">
               <font>
                  <Font size="17.0"/>
               </font>
            </Label>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="42.0" prefWidth="230.0"
                   text="Masukkan Role" textAlignment="CENTER" GridPane.rowIndex="3">
               <font>
                  <Font size="17.0"/>
               </font>
            </Label>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="42.0" prefWidth="230.0"
                   text="Masukkan Nama " textAlignment="CENTER" GridPane.rowIndex="4">
               <font>
                  <Font size="17.0"/>
               </font>
            </Label>
            <PasswordField fx:id="password" prefHeight="37.0" prefWidth="482.0"
                           style="-fx-background-color: #eeeeee; -fx-background-radius: 5;"
                           GridPane.columnIndex="1" GridPane.rowIndex="2">
               <GridPane.margin>
                  <Insets left="15.0" right="15.0"/>
               </GridPane.margin>
            </PasswordField>
            <TextField fx:id="name" prefHeight="36.0" prefWidth="470.0"
                       style="-fx-background-color: #eeeeee; -fx-background-radius: 5;"
                       GridPane.columnIndex="1" GridPane.rowIndex="4">
               <GridPane.margin>
                  <Insets left="15.0" right="15.0"/>
               </GridPane.margin>
            </TextField>
            <TextField fx:id="id" disable="true" prefHeight="36.0" prefWidth="157.0"
                       style="-fx-background-color: #eeeeee; -fx-background-radius: 5;"
                       GridPane.columnIndex="1">
               <GridPane.margin>
                  <Insets left="15.0" right="15.0"/>
               </GridPane.margin>
            </TextField>
            <TextField fx:id="username" prefHeight="36.0" prefWidth="470.0"
                       style="-fx-background-color: #eeeeee; -fx-background-radius: 5;"
                       GridPane.columnIndex="1" GridPane.rowIndex="1">
               <GridPane.margin>
                  <Insets left="15.0" right="15.0"/>
               </GridPane.margin>
            </TextField>
         </GridPane>
         <HBox alignment="CENTER" prefHeight="119.0" prefWidth="409.0">
            <Button fx:id="add" alignment="CENTER" contentDisplay="RIGHT" mnemonicParsing="false"
                    onAction="#add" prefHeight="32.0" prefWidth="114.0"
                    style="-fx-background-color: #005b96; -fx-background-radius: 7; -fx-text-fill: #FFFFFF;"
                    text="Add" textAlignment="RIGHT">
               <HBox.margin>
                  <Insets left="5.0" right="5.0"/>
               </HBox.margin>
            </Button>
            <Button fx:id="update" alignment="CENTER" contentDisplay="RIGHT" mnemonicParsing="false"
                    onAction="#update" prefHeight="32.0" prefWidth="114.0"
                    style="-fx-background-color: #005b96; -fx-background-radius: 7; -fx-text-fill: #FFFFFF;"
                    text="Update" textAlignment="RIGHT">
               <HBox.margin>
                  <Insets left="5.0" right="5.0"/>
               </HBox.margin>
            </Button>
            <Button fx:id="delete" alignment="CENTER" contentDisplay="RIGHT" mnemonicParsing="false"
                    onAction="#delete" prefHeight="32.0" prefWidth="114.0"
                    style="-fx-background-color: #005b96; -fx-background-radius: 7; -fx-text-fill: #FFFFFF;"
                    text="Delete" textAlignment="RIGHT">
               <HBox.margin>
                  <Insets left="5.0" right="5.0"/>
               </HBox.margin>
            </Button>
         </HBox>
      </VBox>
      <TableView prefHeight="536.0" prefWidth="793.0">
         <columns>
            <TableColumn fx:id="colId" prefWidth="45.0" text="ID"/>
            <TableColumn fx:id="colUsername" prefWidth="189.0" text="USERNAME"/>
            <TableColumn fx:id="colPassword" prefWidth="154.0" text="PASSWORD"/>
            <TableColumn fx:id="colName" prefWidth="275.0" text="NAME"/>
            <TableColumn fx:id="colRole" prefWidth="129.0" text="ROLE"/>
         </columns>
      </TableView>
   </HBox>
</VBox>
